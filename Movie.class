public class Movie
{
	
	private string title;
	private char category;
	
	//constructor
	public Movie(string ttl, chat cat)
	{
		title = ttl;
		category = cat;
	}
	
	//copy constructor
	public Movie (Movie object2)
	{
		title = object2.title;
		category = object2.category;
	}

	//getters and setters
	public String getTitle()
	{
		return title;
	}
	public char getCategory()
	{
		return category;
	}
	
	public void set(String ttl, char cat)
	{
		title = ttl;
		category = cat;
	}
	
	public String toString()
	{
		//String representing the movie
		String str = "Movie Title: " + title +
			     "(" + category + ")";
		
		//Return the string
		return str;
	}
}